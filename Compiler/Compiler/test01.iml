program intDiv(in const m:decimal, in const n:int32,
               out const q:int32, out const r:int32)
global
    proc divide(in copy const m:decimal, in copy const n:int32,
                out ref var q:int32, out ref var r:decimal)
    do
        q init := 0;
        r init := m;
        while r >= n do
            q := q + 1;
            r := r - n
        endwhile
    endproc
do
    call divide(m, n, q init, r init)
endprogram