program fibonacci(in const pos:int32, out var fib:int32)
global
	proc fibProc(in copy pos:int32, out copy fib:int32)
		local a : int32; b : int32
	do
        if(pos = 0) then
		    fib init := 1
	    else
		    if(pos = 1) then
			    fib init := 1
			else
			    call fibProc(pos - 2, a init);
				call fibProc(pos - 1, b init);
				fib init := a + b
			endif
		endif
	endproc
do
	call fibProc(pos, fib init)
endprogram
