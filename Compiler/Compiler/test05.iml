program fibonacci(in const pos:int32, out var fib:int32)
global
	proc fibProc(in copy pos:int32, out copy var fib:int32)
        local var a : int32;
              var b : int32
	do
		a init := 0;
		b init := 0;
        if(pos = 0) then
		    fib init := 1
	    else
		    if(pos = 1) then
			    fib init := 1
			else
			    call fibProc(pos - 2, a);
				call fibProc(pos - 1, b);
				fib init := a + b
			endif
		endif
	endproc
do
	call fibProc(pos, fib init)
endprogram
