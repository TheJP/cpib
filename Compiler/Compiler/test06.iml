program fibonacci(in const pos:int32, out var fib:int32)
global
	fun fibFunc(in copy pos:int32) returns var fib : int32
	do
		fib init := 0;
        if(pos = 0) then
		    fib := 1
	    else
		    if(pos = 1) then
			    fib := 1
			else
				fib := fibFunc(pos - 2) + fibFunc(pos - 1)
			endif
		endif
	endfun
do
	fib init := fibFunc(pos)
endprogram
